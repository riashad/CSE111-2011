import java.io.*;

public class Lab10Task06 {
  public static void main(String [] args) {
    
    Scanner k= new Scanner(new File("f:\\Lab08\a.txt"));
    System.out.println("Please Enter 3 number");
    int a;
    int sum;
    
    for(int i=0; i<3; i++){
      a=k.nextInt();
      sum+=a;
    }
    System.out.println(sum);
    
  }
}
